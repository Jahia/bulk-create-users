<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <bulk-create-users j:dependencies="default"
                         j:installedModules="bulk-create-users"
                         j:modulePriority="0"
                         j:moduleType="module"
                         j:title="Bulk Create Users"
                         jcr:primaryType="jnt:module">
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <server-settings-base j:view="serverSettings"
                                  jcr:mixinTypes="jmix:useUILocale"
                                  jcr:primaryType="jnt:template">
               <bulkCreateUsersManageUsers j:applyOn="jnt:globalSettings"
                                           j:defaultTemplate="false"
                                           j:hiddenTemplate="true"
                                           j:priority="100"
                                           j:requiredPermissionNames="adminUsers"
                                           j:titleKey="bulk-create-users.manageUsers"
                                           jcr:mixinTypes="jmix:rbTitle"
                                           jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <manage-users jcr:primaryType="jnt:bulkCreateUsersManageUsers"/>
                  </pagecontent>
               </bulkCreateUsersManageUsers>
            </server-settings-base>
            <site-settings-base j:view="bulk-create-users"
                                jcr:mixinTypes="jmix:useUILocale"
                                jcr:primaryType="jnt:template">
               <bulkCreateUsersManageUsers j:applyOn="jnt:virtualsite"
                                           j:defaultTemplate="false"
                                           j:hiddenTemplate="true"
                                           j:priority="100"
                                           j:requiredPermissionNames="siteAdminUsers"
                                           j:titleKey="bulk-create-users.manageUsers"
                                           jcr:mixinTypes="jmix:rbTitle"
                                           jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <manage-users jcr:primaryType="jnt:bulkCreateUsersManageUsers"/>
                  </pagecontent>
               </bulkCreateUsersManageUsers>
            </site-settings-base>
            <server-settings-base-jahia-anthracite j:view="settingsBootstrap3GoogleMaterialStyle"
                                                   jcr:mixinTypes="jmix:useUILocale"
                                                   jcr:primaryType="jnt:template">
               <bulkCreateUsersManageUsers-jahia-anthracite j:applyOn="jnt:globalSettings"
                                                            j:defaultTemplate="false"
                                                            j:hiddenTemplate="true"
                                                            j:priority="100"
                                                            j:requiredPermissionNames="adminUsers"
                                                            j:titleKey="bulk-create-users.manageUsers"
                                                            jcr:mixinTypes="jmix:rbTitle"
                                                            jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <manage-users j:view="settingsBootstrap3GoogleMaterialStyle"
                                   jcr:mixinTypes="jmix:renderable"
                                   jcr:primaryType="jnt:bulkCreateUsersManageUsers"/>
                  </pagecontent>
               </bulkCreateUsersManageUsers-jahia-anthracite>
            </server-settings-base-jahia-anthracite>
            <site-settings-base-jahia-anthracite j:view="settingsBootstrap3GoogleMaterialStyle"
                                                 jcr:mixinTypes="jmix:useUILocale"
                                                 jcr:primaryType="jnt:template">
               <bulkCreateUsersManageUsers-jahia-anthracite j:applyOn="jnt:virtualsite"
                                                            j:defaultTemplate="false"
                                                            j:hiddenTemplate="true"
                                                            j:priority="100"
                                                            j:requiredPermissionNames="siteAdminUsers"
                                                            j:titleKey="bulk-create-users.manageUsers"
                                                            jcr:mixinTypes="jmix:rbTitle"
                                                            jcr:primaryType="jnt:contentTemplate">
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <manage-users j:view="settingsBootstrap3GoogleMaterialStyle"
                                   jcr:mixinTypes="jmix:renderable"
                                   jcr:primaryType="jnt:bulkCreateUsersManageUsers"/>
                  </pagecontent>
               </bulkCreateUsersManageUsers-jahia-anthracite>
            </site-settings-base-jahia-anthracite>
         </templates>
         <portlets jcr:primaryType="jnt:portletFolder"/>
      </bulk-create-users>
   </modules>
</content>